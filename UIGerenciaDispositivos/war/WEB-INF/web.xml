<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>Cadastrar</servlet-name>
		<servlet-class>sensor4people.pages.index.server.CadastrarImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Cadastrar</servlet-name>
		<url-pattern>/sensor4people.pages.index.Index/Cadastrar</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Buscar</servlet-name>
		<servlet-class>sensor4people.pages.index.server.BuscarImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Buscar</servlet-name>
		<url-pattern>/sensor4people.pages.index.Index/Buscar</url-pattern>
	</servlet-mapping>

</web-app>
